{
  "name": "lessmo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "clean": "expo start -c",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='__tests__' --testPathIgnorePatterns='e2e'",
    "test:integration": "jest --testPathPattern='screens/__tests__'",
    "test:e2e": "jest --testPathPattern='e2e'",
    "test:hooks": "jest --testPathPattern='hooks/__tests__'",
    "test:utils": "jest --testPathPattern='utils/__tests__'",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:appium": "wdio run wdio.conf.js",
    "test:appium:login": "wdio run wdio.conf.js --spec=./tests/appium/login.test.js",
    "test:appium:register": "wdio run wdio.conf.js --spec=./tests/appium/register.test.js",
    "test:appium:event": "wdio run wdio.conf.js --spec=./tests/appium/createEvent.test.js",
    "test:appium:expense": "wdio run wdio.conf.js --spec=./tests/appium/addExpense.test.js",
    "test:appium:summary": "wdio run wdio.conf.js --spec=./tests/appium/summary.test.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.8.4",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/stack": "^7.6.3",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.72.8",
    "eventemitter3": "^5.0.1",
    "expo": "~54.0.25",
    "expo-auth-session": "~7.0.9",
    "expo-constants": "^18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-device": "~8.0.9",
    "expo-file-system": "~19.0.19",
    "expo-image-picker": "~17.0.8",
    "expo-localization": "^17.0.7",
    "expo-notifications": "^0.32.13",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "^15.0.9",
    "firebase": "^12.5.0",
    "i18n-js": "^4.5.1",
    "i18next": "^25.6.2",
    "react": "19.1.0",
    "react-i18next": "^16.3.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-view-shot": "^4.0.3",
    "typescript": "^5.9.3",
    "xlsx": "^0.18.5"
  },
  "private": true,
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@wdio/cli": "^9.20.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/mocha-framework": "^9.20.0",
    "@wdio/spec-reporter": "^9.20.0",
    "appium": "^3.1.1",
    "jest-expo": "^54.0.13",
    "react-test-renderer": "^19.2.0",
    "webdriverio": "^9.20.0"
  }
}
